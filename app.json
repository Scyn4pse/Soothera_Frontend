        "foregroundImage": "./assets/soothera.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [],
      "package": "com.scyn4pse.soothera"
    },
    "extra": {
      "eas": {
        "projectId": "d277f72c-d86e-4223-ac40-e7435fcf97c5"
