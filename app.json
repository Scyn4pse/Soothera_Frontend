{
  "expo": {
    "name": "soothera",
    "slug": "soothera",
    "version": "1.0.0",
    "scheme": "soothera",
    "web": {
      "favicon": "./assets/soothera.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-font"
    ],
    "orientation": "portrait",
